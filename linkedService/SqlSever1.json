{
	"name": "SqlSever1",
	"type": "Microsoft.DataFactory/factories/linkedservices",
	"properties": {
		"description": "independent parameterized linked service ",
		"parameters": {
			"dbName": {
				"type": "string"
			},
			"ServerName": {
				"type": "string"
			},
			"userName": {
				"type": "string"
			},
			"passoword": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "SqlServer",
		"typeProperties": {
			"connectionString": "Integrated Security=False;Data Source=@{linkedService().ServerName};Initial Catalog=@{linkedService().dbName};User ID=@{linkedService().userName};Password=@{linkedService().passoword}"
		}
	}
}